{
    "name": "dragonofmercy/custom-ai-solution",
    "description": "AI Solution provider using custom service for Spatie Ignition Error Solution",
    "keywords" : ["error", "page", "laravel", "openrouter", "openai", "groq"],
    "authors" : [{
        "name" : "DragonOfMercy",
        "email" : "dragonofmercy@gmail.com",
        "role" : "Developer"
    }],
    "homepage" : "https://github.com/dragonofmercy/custom-ai-solution",
    "license" : "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/support": ">=10.0",
        "openai-php/client": ">=0.10.3",
        "spatie/laravel-ignition": ">=2.9"
    },
    "autoload": {
        "psr-4": {
            "Dragon\\CustomAi\\": "src/"
        }
    },
    "minimum-stability" : "dev",
    "prefer-stable" : true,
    "extra": {
        "laravel": {
            "providers": [
                "Dragon\\CustomAi\\CustomAiServiceProvider"
            ]
        }
    }
}